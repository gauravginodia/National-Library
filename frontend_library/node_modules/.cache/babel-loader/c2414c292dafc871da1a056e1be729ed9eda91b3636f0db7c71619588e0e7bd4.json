{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from \"../components/NavBar.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"RegisterUser\",\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\",\n      agreedToTerms: false\n    };\n  },\n  methods: {\n    registerUser() {\n      if (!this.validateData()) {\n        return;\n      }\n      axios.post(\"http://127.0.0.1:5000/api/signup\", {\n        name: this.name,\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        console.log(\"Response from backend:\", response.data.message);\n        this.$router.push('/');\n      }).catch(error => {\n        if (error.response && error.response.status === 400) {\n          alert(error.response.data.error);\n        } else {\n          alert(\"Unexpected Error Ocurred.Try Again Later\");\n          console.error(\"Error:\", error);\n        }\n      });\n    },\n    // validation\n    validateData() {\n      if (this.name === \"\" || this.email === \"\" || this.password === \"\" || this.confirmPassword === \"\") {\n        alert(\"Please fill in all fields\");\n        return false;\n      }\n      if (this.password !== this.confirmPassword) {\n        alert(\"Passwords do not match\");\n        return false;\n      }\n      if (!this.agreedToTerms) {\n        alert(\"Please agree to terms and conditions\");\n        return false;\n      }\n      return true;\n    }\n  }\n};","map":{"version":3,"names":["NavBar","axios","name","components","data","email","password","confirmPassword","agreedToTerms","methods","registerUser","validateData","post","then","response","console","log","message","$router","push","catch","error","status","alert"],"sources":["C:\\Users\\vijay\\OneDrive\\Desktop\\National-Library-LMS\\Library_Code\\frontend_library\\src\\views\\RegisterUser.vue"],"sourcesContent":["<template>\n  <div>\n    <NavBar />\n    <div class=\"container\">\n      <h1 class=\"heading\">Sign Up</h1>\n      <form @submit.prevent=\"registerUser\">\n        <div class=\"form-group\">\n          <label for=\"inputName\">Username</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputName\"\n            v-model=\"name\"\n          />  \n        </div>\n        <div class=\"form-group\">\n          <label for=\"inputEmail\">Email address</label>\n          <input\n            type=\"email\"\n            class=\"form-control\"\n            id=\"inputEmail\"\n            aria-describedby=\"emailHelp\"\n            v-model=\"email\"\n          />\n    \n        </div>\n        <div class=\"form-group\">\n          <label for=\"inputPassword\">Password</label>\n          <input\n            type=\"password\"\n            class=\"form-control\"\n            id=\"inputPassword\"\n            v-model=\"password\"\n          />\n        </div>\n        <div class=\"form-group\">\n          <label for=\"confirmPassword\">Confirm Password</label>\n          <input\n            type=\"password\"\n            class=\"form-control\"\n            id=\"confirmPassword\"\n            v-model=\"confirmPassword\"\n          />\n        </div>\n        <div class=\"form-check\">\n          <input\n            type=\"checkbox\"\n            class=\"form-check-input\"\n            id=\"agreeTerms\"\n            v-model=\"agreedToTerms\"\n          />\n          <label class=\"form-check-label\" for=\"agreeTerms\">\n            I agree to Terms and Conditions\n          </label>\n        </div>\n        <div class=\"button\">\n          <button type=\"submit\" class=\"btn btn-primary\">Register</button>\n        </div>\n        <div class=\"have-account\">\n          <small class=\"mt-3\">\n            Already have an account?\n            <router-link to=\"/\">Login</router-link>\n          </small>\n        </div>\n      </form>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport NavBar from \"../components/NavBar.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"RegisterUser\",\n  components: {\n    NavBar,\n  },\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\",\n      agreedToTerms: false,\n    };\n  },\n  methods: {\n    registerUser() {\n      if (!this.validateData()) {\n        return;\n      }\n\n      axios.post(\"http://127.0.0.1:5000/api/signup\", {\n          name: this.name,\n          email: this.email,\n          password: this.password,\n        })\n        .then((response) => {\n          console.log(\"Response from backend:\", response.data.message);\n          this.$router.push('/');\n        })\n        .catch((error) => {\n          if (error.response && error.response.status === 400) {\n            alert(error.response.data.error);\n          } else {\n            alert(\"Unexpected Error Ocurred.Try Again Later\");\n            console.error(\"Error:\", error);\n          }\n        });\n    },\n    // validation\n    validateData() {\n      if (\n        this.name === \"\" ||\n        this.email === \"\" ||\n        this.password === \"\" ||\n        this.confirmPassword === \"\"\n      ) {\n        alert(\"Please fill in all fields\");\n        return false;\n      }\n      if (this.password !== this.confirmPassword) {\n        alert(\"Passwords do not match\");\n        return false;\n      }\n      if (!this.agreedToTerms) {\n        alert(\"Please agree to terms and conditions\");\n        return false;\n      }\n      return true;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  width: 40%;\n  margin-top: 50px;\n  border: 1px solid #000;\n  border-radius: 10px;\n  padding: 20px;\n  font-family: Raleway, sans-serif;\n  background-color: #333;\n  color:white;\n}\n\n.heading {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.btn-primary {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  text-align: center;\n  margin-top: 15px;\n  width: 30%;\n  padding: 10px;\n  border-radius: 10px;\n  background-color: #000;\n  border: none;\n  color: #fff;\n  font-size: 20px;\n  cursor: pointer;\n}\n\n.button {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  text-align: center;\n}\n\n.have-account {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  text-align: center;\n}\n</style>\n"],"mappings":";AAuEA,OAAOA,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,IAAI,EAAE,EAAE;MACRG,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACC,YAAY,CAAC,CAAC,EAAE;QACxB;MACF;MAEAV,KAAK,CAACW,IAAI,CAAC,kCAAkC,EAAE;QAC3CV,IAAI,EAAE,IAAI,CAACA,IAAI;QACfG,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EACAO,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAAC,wBAAwB,EAAEF,QAAQ,CAACV,IAAI,CAACa,OAAO,CAAC;QAC5D,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACxB,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChB,IAAIA,KAAK,CAACP,QAAO,IAAKO,KAAK,CAACP,QAAQ,CAACQ,MAAK,KAAM,GAAG,EAAE;UACnDC,KAAK,CAACF,KAAK,CAACP,QAAQ,CAACV,IAAI,CAACiB,KAAK,CAAC;QAClC,OAAO;UACLE,KAAK,CAAC,0CAA0C,CAAC;UACjDR,OAAO,CAACM,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;QAChC;MACF,CAAC,CAAC;IACN,CAAC;IACD;IACAV,YAAYA,CAAA,EAAG;MACb,IACE,IAAI,CAACT,IAAG,KAAM,EAAC,IACf,IAAI,CAACG,KAAI,KAAM,EAAC,IAChB,IAAI,CAACC,QAAO,KAAM,EAAC,IACnB,IAAI,CAACC,eAAc,KAAM,EAAC,EAC1B;QACAgB,KAAK,CAAC,2BAA2B,CAAC;QAClC,OAAO,KAAK;MACd;MACA,IAAI,IAAI,CAACjB,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1CgB,KAAK,CAAC,wBAAwB,CAAC;QAC/B,OAAO,KAAK;MACd;MACA,IAAI,CAAC,IAAI,CAACf,aAAa,EAAE;QACvBe,KAAK,CAAC,sCAAsC,CAAC;QAC7C,OAAO,KAAK;MACd;MACA,OAAO,IAAI;IACb;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}